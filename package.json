{
  "name": "sengi",
  "version": "4.0.0",
  "type": "module",
  "description": "Provides a RESTful interface to a document-based database.",
  "main": "./index.js",
  "exports": "./index.js",
  "scripts": {
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js && standard",
    "reinstall": "rm -rf node_modules && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karlhulme/sengi.git"
  },
  "author": "Karl Hulme <karlhulme@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karlhulme/sengi/issues"
  },
  "homepage": "https://github.com/karlhulme/sengi#readme",
  "devDependencies": {
    "acorn": "^7.1.1",
    "jest": "^25.1.0",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "check-types": "^10.1.2",
    "fast-json-patch": "^2.1.0",
    "moment": "^2.24.0",
    "sengi-validation": "^1.6.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 0
      }
    },
    "testEnvironment": "node"
  },
  "standard": {
    "ignore": [
      "/docs/"
    ]
  }
